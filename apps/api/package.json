  "license": "MIT",
  "devDependencies": {
    "@snowball-tools/eslint-config": "workspace:*",
    "@snowball-tools/typescript-config": "workspace:*",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.4",
    "nodemon": "^3.1.10",
    "typescript": "~5.6.2"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.13.8",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "express": "^4.21.2",
