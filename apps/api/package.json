{
  "name": "api",
  "version": "0.0.0",
  "description": "api via express",
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/index.js\"",
    "build": "tsc",
    "serve": "node dist/index.js",
    "check-types": "tsc --noEmit",
    "lint": "eslint . --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "author": "Snowball Tools Inc.",
  "license": "MIT",
  "devDependencies": {
    "@snowball-tools/eslint-config": "workspace:*",
    "@snowball-tools/typescript-config": "workspace:*",
    "@types/express": "^5.0.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.4",
    "nodemon": "^3.1.10",
    "typescript": "~5.6.2"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.5",
    "express": "^4.21.2",
    "mysql2": "^3.15.0"
  },
  "packageManager": "pnpm@9.0.0"
}
